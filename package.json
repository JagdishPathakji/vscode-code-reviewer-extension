{
  "name": "code-reviewer-by-jagdish",
  "displayName": "AI Code Reviewer by Jagdish",
  "description": "AI-powered folder code review",
  "version": "0.0.6",
  "publisher": "JagdishPathakji",
  "engines": {
    "vscode": "^1.107.0"
  },
  "icon": "icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/JagdishPathakji/vscode-code-reviewer-extension.git"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:code-reviewer-by-jagdish.reviewFolder"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "code-reviewer-by-jagdish.reviewFolder",
        "title": "AI Code Reviewer by Jagdish"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "code-reviewer-by-jagdish.reviewFolder",
          "when": "resource",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "@types/vscode": "^1.107.0",
    "@vscode/test-cli": "^0.0.12",
    "@vscode/test-electron": "^2.5.2",
    "eslint": "^9.39.1"
  },
  "dependencies": {
    "@google/genai": "^1.34.0",
    "@langchain/google-genai": "^2.1.3",
    "dotenv": "^17.2.3",
    "ollama": "^0.6.3",
    "zod": "^4.2.1"
  },
  "type": "commonjs"
}
